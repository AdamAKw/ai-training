{
  "name": "application",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cypress:open": "xvfb-run -a cypress open",
    "cypress:run": "xvfb-run -a cypress run --headless",
    "cypress:run:headless": "xvfb-run -a cypress run --headless --browser electron",
    "cypress:run:chrome": "xvfb-run -a cypress run --headless --browser chrome",
    "cypress:run:firefox": "xvfb-run -a cypress run --headless --browser firefox",
    "cypress:verify": "xvfb-run -a cypress verify",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run:headless",
    "test:e2e:chrome": "start-server-and-test dev http://localhost:3000 cypress:run:chrome",
    "test:e2e:firefox": "start-server-and-test dev http://localhost:3000 cypress:run:firefox",
    "test:e2e:open": "start-server-and-test dev http://localhost:3000 cypress:open",
    "test:e2e:ci": "DISPLAY=:99 xvfb-run -s '-screen 0 1024x768x24' start-server-and-test dev http://localhost:3000 'cypress run --headless --browser electron'",
    "test": "npm run cypress:run:headless",
    "build:test": "next build && start-server-and-test http://localhost:3000",
    "start:test": "start-server-and-test http://localhost:3000"
  },
